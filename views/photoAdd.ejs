<%- include('partials/_header') -%>
    <%- include('partials/_navigation') -%>
	<div class="all-title-box">
		<div class="container text-center">
			<h1>Add Photo<span class="m_1">Lorem Ipsum dolroin gravida nibh vel velit.</span></h1>
		</div>
	</div>
	
    <div id="contact" class="section wb">
        <div class="container">
            <div class="section-title text-center">
                <h3>Add Photo for Live Lesson</h3>
                <p class="lead">Please Upload 3 Photos of Yours.</p>
            </div><!-- end title -->

            <div class="row">
                <div class="offset-1 col-xl-10 col-md-10 col-sm-10">

                    <% if(flashMessages) { %>

                        <% if(flashMessages.success) { %>
                            <div class="alert alert-success">
                                <%= flashMessages.success %>
                            </div>
                        <% } else if (flashMessages.error) { %>
                            <div class="alert alert-danger">
                                <%= flashMessages.error %>
                            </div>
                        <% } %>                    
                    <% } %>
                    <div class="contact_form">
                        <div id="message"></div>
                        <form  method="POST" action="/upload" class="tm-contact-form" encType="multipart/form-data">
                            <div class="row row-fluid">
                                <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                    <input type="file" name="image1" style="background-color: #333333;" class="form-control rounded-0" accept="image/png, image/jpg, image/jpeg" required>
                                </div>
                                <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                    <input type="file" name="image2" style="background-color: #333333;" class="form-control rounded-0" accept="image/png, image/jpg, image/jpeg" required>
                                </div>
                                <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                    <input type="file" name="image3" style="background-color: #333333;" class="form-control rounded-0" accept="image/png, image/jpg, image/jpeg" required>
                                </div>
                                <div class="text-center pd">
                                    <button type="submit" value="SEND" id="submit" class="btn btn-light btn-radius btn-brd grd1 btn-block">SEND</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div><!-- end col -->
            </div><!-- end row -->
        </div><!-- end container -->
    </div><!-- end section -->
    <%- include('partials/_footer') -%>